{
    "name": "SpinIt",
    "description": "Makes things spin.",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["contextMenus"],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "css": ["spinning.css"],
        "js": ["content.js"]
    }],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}
